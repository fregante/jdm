{
	"name": "Just Delete Me",
	"version": "0.0.0",
	"description": "Find out how difficult it is to delete your account from websites",
	"homepage_url": "https://github.com/fregante/jdm",
	"manifest_version": 3,
	"minimum_chrome_version": "100",
	"icons": {
		"128": "icon.png"
	},
	"permissions": [
		"action",
		"activeTab",
		"alarms",
		"declarativeContent"
	],
	"action": {
		"default_icon": {
			"32": "icons/unknown.png"
		}
	},
	"background": {
		"service_worker": "background.js",
		"type": "module"
	}
}
